            <attDef ident="n" mode="change">
              <desc>Provides a positive or negative numeric value for the vertical position of certain 'glued on' sections on the page.</desc>
              <datatype minOccurs="1" maxOccurs="unbounded">
                <rng:ref xmlns:rng="http://relaxng.org/ns/structure/1.0" name="data.numeric"/>
              </datatype>
            </attDef>
            
            <attDef ident="rend" mode="change">
              <desc>Set to "glued" if the choice element is used to ernder a passage that is glued on top (rather than to the left or bottom) of the original page. In this case orig element holds the text 
                    underneath the glued on section.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="glued"/>
              </valList>
            </attDef>
            
            <attDef ident="type" mode="change">
              <desc>Marks the change as either 'editorial' (prompted by an editor) or 'authorial' (prompted by the author herself). [not used anymore?]</desc>
              <valList type="closed" mode="replace">
                <valItem ident="editorial"/>
                <valItem ident="authorial"/>
                <valItem ident="emendation"/>
              </valList>
