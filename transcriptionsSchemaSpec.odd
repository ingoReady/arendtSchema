            <attDef ident="hand" mode="change">
              <desc>Deletion was marked by handwriting (crossed out); @hand specifies the person responsible.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="#HA2"/>
                <valItem ident="unknown"/>
                <valItem ident="edZionism"/>
              </valList>
            </attDef>
            
            <attDef ident="n" mode="delete" /><!--was used to tell extent of large deletion, but no longer needed.-->
              
            <attDef ident="rend" mode="change" usage="req">
              <desc>The way the deletion is rendered in the source. "overwritten" means that a character was overwritten by hand with a new character; used in the mod element. "large" means that the deletion
                    spans multiple lines (e.g. a big cross on the page); used in conjunction with @n. "parenthesed" means an alternative deletion where text is omitted but not crossed out.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="strikethrough"/>
                <valItem ident="strikethroughLarge"/>
                <valItem ident="overtyped"/>
                <valItem ident="overwritten"/>
                <valItem ident="large"/>
                <valItem ident="erased"/>
                <valItem ident="altDeletion"/>
                <valItem ident="altDeletion2"/>
                <valItem ident="altDeletionX"/>
              </valList>
            </attDef>
            
            <attDef ident="type" mode="change" usage="req">
              <desc>Type of deletion. 
                    "recoverable": crossed out contents can be retrieved.
                    "irrecoverable": crossed out contents cannot be retrieved. 
                    "lowerCase": the first lower-case 
                    character of a word has been crossed out in order
                    to indicate its change to upper-case.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="recoverable"/>
                <valItem ident="irrecoverable"/>
                <valItem ident="lowerCase"/>
              </valList>
            </attDef>
            
            <attDef ident="subtype" mode="change">
              <desc>Subtype of deletion. Used to give rendition in constituted version.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="displayConst"/>
              </valList>
            </attDef>
            
          </attList>
        </elementSpec>
        
