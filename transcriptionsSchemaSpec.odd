            <attDef ident="xml:id" mode="delete"/>
            <attDef ident="xml:lang" mode="delete"/>
            <attDef ident="xml:space" mode="delete"/>
            
            <attDef ident="place" mode="change" usage="req">
              <desc>The place of the addition relative to caret position. "Superimposed" is only used with element mod.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="inline"/>
                <valItem ident="above"/>
                <valItem ident="below"/>
                <valItem ident="superimposed"/>
                <valItem ident="leftmargin"/>
                <valItem ident="rightmargin"/>
                <valItem ident="float"/>
              </valList>
            </attDef>
            
            <attDef ident="hand" mode="change">
              <desc>Marks the addition as handwritten. Reference to an xml:id specifies responsible person.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="#HA2"/>
                <valItem ident="unknown"/>
                <valItem ident="edZionism"/>
              </valList>
            </attDef>
            
            <attDef ident="rend" mode="change">
              <desc>The way the addition is rendered in the source: "locHorizontal" means that the addition is located left or right from caret position. @n will provide the value to determine how many characters.
                     "Insertionmark" means that the addition is provided with an insertionmark in the source. Use only with @place="left/rightmargin". "Float" is for additions that are placed outside textflow.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="locHorizontal"/>
                <valItem ident="insertionmark"/>
                <valItem ident="float"/><!--depricated-->
                <valItem ident="floatNormal"/><!--depricated-->
                <valItem ident="floatSingle"/><!--depricated-->
              </valList>
            </attDef>
            
            <attDef ident="style" mode="change">
              <desc>Used to provide character information on how the insertion mark should be rendered.</desc>
              <valList type="open" mode="replace">
                <valItem ident="/"/>
              </valList>
            </attDef>
            
            <!-- Formerly used to mark pages glued on left margin, but now discarded: information moved to @type. -->
            <attDef ident="subtype" mode="delete" />
              
            <attDef ident="type" mode="change">
              <desc>Specifies the type of the addition. "Insertionmark" means that the whole add element is considered as an insertionmark (as opposed to @rend='insertionmark'). Used when above/below additions
                    come with an insertion mark. "noD" means 'no display': the element will not be rendered in constituted versions of the text.</desc>
              <valList type="closed" mode="replace">
                <valItem ident="insertionmark"/>
                <valItem ident="noD"/>
                <valItem ident="reversed"/>
                <valItem ident="separationmark"/>
                <valItem ident="noSpace"/>
                <valItem ident="addPage"/>
                <valItem ident="attachedNote"/>
                <valItem ident="insertFromPage"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        
